      <TextField fx:id="amount" layoutX="38.0" layoutY="198.0" opacity="0.9" prefHeight="26.0" prefWidth="266.0" promptText="Введите кол-во купленных запчастей" />
      <Button fx:id="addButton" layoutX="234.0" layoutY="497.0" mnemonicParsing="false" opacity="0.8" prefHeight="82.0" prefWidth="139.0" stylesheets="@ButtonSettings.css" text="Подтвердить" />
